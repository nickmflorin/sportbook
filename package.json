{
  "name": "sportbook",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=19"
  },
  "engineStrict": true,
  "scripts": {
    "requirements-check": "ts-node --project=./tsconfig.json  src/application/support/enforce-node-version.ts",
    "setup-env": "npm run requirements-check && prisma generate",
    "preinstall": "npm run requirements-check",
    "predev": "npm run setup-env",
    "prestart": "npm run setup-env",
    "prebuild": "npm run setup-env",
    "prebuild-local": "npm run setup-env",
    "pretest": "npm run setup-env",
    "prelint": "npm run setup-env",
    "preeslint-fix": "npm run setup-env",
    "pretsc": "npm run setup-env",
    "postinstall": "prisma generate",
    "build": "next build",
    "build-local": "rm -rf ./.next && next build",
    "dev": "next dev",
    "lint": "next lint && stylelint \"./src/**/*.{scss,css}\" && prettier -c .",
    "eslint-fix": "npx eslint . --fix",
    "start": "next start",
    "test": "jest --watchAll  --silent=false",
    "test:ci": "jest --ci",
    "tsc": "tsc --project ./tsconfig.json",
    "tsc:watch": "tsc -w"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.10.14",
    "@clerk/nextjs": "^4.21.13",
    "@mantine/core": "^6.0.15",
    "@mantine/dates": "^6.0.15",
    "@mantine/dropzone": "^6.0.15",
    "@mantine/form": "^6.0.15",
    "@mantine/hooks": "^6.0.15",
    "@mantine/next": "^6.0.15",
    "@mantine/ssr": "^6.0.15",
    "@prisma/client": "^4.14.0",
    "@t3-oss/env-nextjs": "^0.3.1",
    "@tabler/icons-react": "^2.23.0",
    "@tanstack/react-query": "^4.29.7",
    "@trpc/client": "^10.33.0",
    "@trpc/next": "^10.33.0",
    "@trpc/react-query": "^10.33.0",
    "@trpc/server": "^10.33.0",
    "classnames": "^2.3.2",
    "dotenv-cli": "^7.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lodash.uniq": "^4.5.0",
    "luxon": "^3.3.0",
    "mantine-datatable": "^2.8.2",
    "next": "^13.4.7",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.25",
    "prettier": "^3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.63.6",
    "semver": "^7.5.3",
    "superjson": "1.12.2",
    "typescript": "5.1.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.2",
    "@types/lodash.uniq": "^4.5.7",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.16.0",
    "@types/npm": "^7.19.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "cross-env": "^7.0.3",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest-expect-message": "^1.1.3",
    "jest-runner-eslint": "^2.1.0",
    "jest-runner-prettier": "^1.0.0",
    "jest-runner-stylelint": "^2.3.7",
    "prettier-plugin-prisma": "^4.13.0",
    "prisma": "^4.14.0",
    "stylelint": "^15.10.1",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-scss": "^5.0.1",
    "stylelint-webpack-plugin": "^4.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "utility-types": "^3.10.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.15.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "dotenv -e .env -e .env.development -e .env.local -e .env.development.local cross-env NODE_ENV=development npx tsx --tsconfig ./tsconfig.json src/prisma/seed.ts"
  }
}
