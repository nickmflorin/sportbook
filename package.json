{
  "name": "sportbook",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "engineStrict": true,
  "scripts": {
    "requirements-check": "ts-node --compilerOptions='{\"module\":\"commonjs\"}' src/application/support/enforce-node-version.ts",
    "setup-env": "npm run requirements-check && npm run prisma-generate",
    "preinstall": "npm run requirements-check",
    "predev": "npm run setup-env",
    "prestart": "npm run setup-env",
    "prebuild": "npm run setup-env",
    "prebuild-local": "npm run setup-env",
    "pretest": "npm run setup-env",
    "prelint": "npm run setup-env",
    "preeslint-fix": "npm run setup-env",
    "pretsc": "npm run setup-env",
    "postinstall": "npm run prisma-generate",
    "build": "next build",
    "build-local": "rm -rf ./.next && next build",
    "dev": "next dev",
    "lint": "next lint && stylelint \"./src/**/*.{scss,css}\" && prettier -c .",
    "eslint-fix": "npx eslint . --fix",
    "start": "next start",
    "test": "jest --watchAll  --silent=false",
    "test:ci": "jest --ci",
    "tsc": "tsc --project ./tsconfig.json",
    "tsc:watch": "tsc -w",
    "prisma-generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma-migrate-dev": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "prisma-migrate": "dotenv -e .env -e .env.development -e .env.local -e .env.development.local npm run prisma-migrate-dev"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.10.14",
    "@clerk/nextjs": "^4.17.3",
    "@mantine/core": "^6.0.15",
    "@mantine/dates": "^6.0.15",
    "@mantine/dropzone": "^6.0.15",
    "@mantine/form": "^6.0.15",
    "@mantine/hooks": "^6.0.15",
    "@mantine/next": "^6.0.15",
    "@mantine/ssr": "^6.0.15",
    "@prisma/client": "^4.14.0",
    "@t3-oss/env-nextjs": "^0.3.1",
    "@tabler/icons-react": "^2.23.0",
    "@tanstack/react-query": "^4.29.7",
    "@trpc/client": "^10.26.0",
    "@trpc/next": "^10.26.0",
    "@trpc/react-query": "^10.26.0",
    "@trpc/server": "^10.26.0",
    "dotenv-cli": "^7.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "next": "13.4.7",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.63.6",
    "semver": "^7.5.3",
    "superjson": "1.12.2",
    "typescript": "5.1.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest-expect-message": "^1.1.3",
    "jest-runner-eslint": "^2.1.0",
    "jest-runner-prettier": "^1.0.0",
    "jest-runner-stylelint": "^2.3.7",
    "prettier-plugin-prisma": "^4.13.0",
    "prisma": "^4.14.0",
    "stylelint": "^15.9.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-prettier": "^3.0.0",
    "stylelint-webpack-plugin": "^4.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.15.0"
  }
}
