{
  "name": "sportbook",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=19"
  },
  "engineStrict": true,
  "scripts": {
    "requirements-check": "ts-node --project=./tsconfig.json  src/application/support/enforce-node-version.ts",
    "setup-env": "env-cmd -r ./.env-emdrc.js -e all npm run requirements-check && prisma generate",
    "preinstall": "env-cmd -r ./.env-emdrc.js -e all npm run requirements-check",
    "postinstall": "prisma generate",
    "prebuild": "npm run setup-env",
    "analyze-build": "ANALYZE_BUILD=true npm run build",
    "build": "next build",
    "prebuild-local": "npm run setup-env",
    "build-local": "rm -rf ./.next && next build",
    "predev": "npm run setup-env",
    "dev": "next dev",
    "prestart": "npm run setup-env",
    "start": "next start",
    "prelint": "npm run setup-env",
    "lint": "next lint && stylelint \"./src/**/*.{scss,css}\"",
    "preeslint-fix": "npm run requirements-check",
    "eslint-fix": "npx eslint . --fix",
    "pretest": "npm run setup-env",
    "test": "jest --watchAll  --silent=false",
    "test:ci": "jest --ci",
    "pretsc": "npm run setup-env",
    "tsc": "tsc --project ./tsconfig.json",
    "pretsc:watch": "npm run setup-env",
    "tsc:watch": "tsc -w"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.10.14",
    "@clerk/nextjs": "^4.21.13",
    "@fortawesome/fontawesome-pro": "^6.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/pro-regular-svg-icons": "^6.4.0",
    "@fortawesome/pro-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fortawesome/sharp-regular-svg-icons": "^6.4.0",
    "@fortawesome/sharp-solid-svg-icons": "^6.4.0",
    "@mantine/core": "^6.0.15",
    "@mantine/dates": "^6.0.15",
    "@mantine/dropzone": "^6.0.15",
    "@mantine/form": "^6.0.15",
    "@mantine/hooks": "^6.0.15",
    "@mantine/next": "^6.0.15",
    "@mantine/ssr": "^6.0.15",
    "@prisma/client": "^4.14.0",
    "@t3-oss/env-nextjs": "^0.3.1",
    "babel-plugin-macros": "^3.1.0",
    "classnames": "^2.3.2",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "env-cmd": "^10.1.0",
    "lodash.chunk": "^4.2.0",
    "lodash.difference": "^4.5.0",
    "lodash.findindex": "^4.6.0",
    "lodash.intersection": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.keys": "^4.2.0",
    "lodash.pickby": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "luxon": "^3.3.0",
    "mantine-datatable": "^2.8.2",
    "next": "^13.4.7",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.63.6",
    "semver": "^7.5.3",
    "superjson": "^1.12.4",
    "swr": "^2.2.0",
    "typescript": "5.1.6",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/classnames": "^2.3.1",
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.2",
    "@types/lodash.chunk": "^4.2.7",
    "@types/lodash.difference": "^4.5.7",
    "@types/lodash.findindex": "^4.6.7",
    "@types/lodash.intersection": "^4.4.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.keys": "^4.2.7",
    "@types/lodash.pickby": "^4.6.7",
    "@types/lodash.uniq": "^4.5.7",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.16.0",
    "@types/npm": "^7.19.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "cross-env": "^7.0.3",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-prettier": "~5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-expect-message": "^1.1.3",
    "jest-runner-eslint": "^2.1.0",
    "jest-runner-prettier": "^1.0.0",
    "jest-runner-stylelint": "^2.3.7",
    "prisma": "^4.14.0",
    "stylelint": "^15.0.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-scss": "~5.0.1",
    "stylelint-webpack-plugin": "~4.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "utility-types": "^3.10.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.15.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "dotenv -e .env -e .env.development -e .env.local -e .env.development.local cross-env NODE_ENV=development npx tsx --tsconfig ./tsconfig.json src/prisma/seed.ts"
  }
}
